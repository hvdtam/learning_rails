<div class="row">
  <div class="col-sm-6">
    <h1>Provinces</h1>
  </div>

  <div class="col-sm-6 text-right">
    <%#= link_to new_television_path, class: 'btn btn-primary' do %>
    Add New Provinces
    <%# end %>
  </div>
</div>
<div class="row">
  <div class="col-sm-10">
    <div class="table-responsive">
      <table class="table table-striped table-bordered table-hover">
        <thead>
        <tr>

          <th>Name</th>
          <th>Code</th>
          <th>Division Type</th>
          <th>Phone Code</th>
          <th>Code Name</th>

        </tr>
        </thead>

        <tbody>
        <% @provinces.each do |province| %>
          <%= content_tag :tr, id: dom_id(province), class: dom_class(province) do %>

            <!--          <td><%#= link_to province.name, province %></td>-->


            <td><%= province.name %></td>
            <td><%= province.code %></td>
            <td><%= province.division_type %></td>
            <td><%= province.phone_code %></td>
            <td><%= province.codename %></td>


          <% end %>
        <% end %>
        </tbody>
      </table>
    </div>

  </div>
</div>